/* MACHINE-GENERATED ! */

frame pad {
	set Px = 0.5mm

	set Py = 0.5mm

	set cname = col+1

	set e = 1mm

	__0: vec @(col*e-Px/2, row*-e-Py/2)
	__1: vec .(0mm, Py)
	__2: vec __0(Px, 0mm)
	pad "$rname$cname" __1 .
}

frame inner {
	loop col = 2, 3

	loop enable = 1, inner

	frame pad @
}

frame last {
	loop col = 4, 5

	frame pad @
}

frame first {
	loop col = 0, 1

	frame pad @
}

part "Fake_BGA"
table
    { row, rname, inner }
    { 0, "A", 1 }
    { 1, "B", 1 }
    { 2, "C", 0 }
    { 3, "D", 0 }
    { 4, "E", 1 }
    { 5, "F", 1 }

frame last @
frame first @
frame inner @
measy pad.__0 -> pad.__1 0.2mm
measy pad.__0 -> pad.__0 0.5mm
measx pad.__0 -> pad.__2 -0.3mm
measx pad.__0 -> pad.__0 -0.6mm
measy pad.__0 >> pad.__1 0.8mm
measx pad.__0 >> pad.__2 -0.9mm
